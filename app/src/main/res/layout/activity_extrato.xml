<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundo2"
    tools:context="comviewzonazul.google.httpssites.zonazul.cliente.gui.ExtratoActivity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Historico de Compras"
        android:textColor="@color/colorAccent2"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="16dp"
        android:textSize="25dp"
        android:layout_marginTop="23dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />



    <Button
        android:id="@+id/button5"
        android:layout_width="195dp"
        android:layout_height="48dp"
        android:text="Gerar historico"
        tools:layout_editor_absoluteX="84dp"
        tools:layout_editor_absoluteY="219dp"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:onClick="historico"/>
</RelativeLayout>
